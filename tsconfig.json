{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "module": "es6",
      "target": "es5",
      "allowJs": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
          "s-expression": ["node_modules/s-expression/index.js"],
          "repl": ["node_modules/@types/node/repl.d.ts"]
      },
      "noEmitOnError": true,
      "strict": true,
      "types": [
        "jest"
      ],
      "typeRoots": [
